---
const { title } = Astro.props
// import { BrowserRouter } from "react-router-dom"
// import { SnackbarProvider } from "notistack"
// import { SnackbarUtilsConfigurator } from "../utils/customHooks/useSnackBarUtils"
// import { HelmetProvider } from "react-helmet-async"
import Header from "../components/Header/Header"
import Footer from "../components/Footer/Footer"
import BurgerModal from "../components/Modal/BurgerModal"
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta
      name="description"
      content="Digital Artist services, accessible for everyone."
    />
		<meta title={title} />
  </head>
</html>
  <body>
<main class="m-0 relative">
	<header>
		<Header client:load/>
	</header>
	<BurgerModal client:load />
						<slot />
  <footer>
    <div><Footer page={"main"} /></div>
  </footer>
</main>
</body>
<style>
  body {
    font-family: "Josefin Sans Variable", sans-serif;
  }
</style>